{
  "name": "@vizualmodel/vmblu-runtime",
  "version": "0.1.1",
  "description": "vmblu runtime javascript library for running node-based apps",
  "license": "Apache-2.0",
  "author": "Vizual Model",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup src/index.js --format esm,cjs --out-dir dist --sourcemap --splitting false --keep-names",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
      "eslint": "^9.36.0",
      "@eslint/js": "^9.36.0",
      "eslint-config-prettier": "^10.1.8",
      "globals": "^16.4.0",
      "prettier": "^3.6.2",
      "typescript":"^5.x",
      "tsup": "^8.5.0"
  }
}
